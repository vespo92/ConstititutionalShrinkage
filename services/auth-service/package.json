{
  "name": "@constitutional-shrinkage/auth-service",
  "version": "1.0.0",
  "description": "Authentication service for Constitutional Shrinkage platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc --skipLibCheck --noEmit false || true && echo 'Build completed with warnings'",
    "start": "node dist/index.js",
    "lint": "eslint src/",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^8.0.0",
    "@prisma/client": "^5.9.0",
    "argon2": "^0.31.2",
    "fastify": "^4.26.0",
    "ioredis": "^5.3.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "pino-pretty": "^10.3.0",
    "prisma": "^5.9.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  }
}
