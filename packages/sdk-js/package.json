{
  "name": "@constitutional/sdk",
  "version": "1.0.0",
  "description": "Official JavaScript/TypeScript SDK for the Constitutional Platform Public API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "cross-fetch": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0"
  },
  "keywords": [
    "constitutional",
    "governance",
    "api",
    "sdk",
    "democracy"
  ],
  "author": "Constitutional Platform",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vespo92/ConstititutionalShrinkage.git",
    "directory": "packages/sdk-js"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
