{
  "name": "@constitutional-shrinkage/data-connectors",
  "version": "1.0.0",
  "description": "Data connectors for government APIs including Congress.gov, Census Bureau, FEC, and more",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "lint": "eslint src/",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./congress-gov": {
      "import": "./dist/sources/congress-gov/index.js",
      "types": "./dist/sources/congress-gov/index.d.ts"
    },
    "./census": {
      "import": "./dist/sources/census/index.js",
      "types": "./dist/sources/census/index.d.ts"
    },
    "./fec": {
      "import": "./dist/sources/fec/index.js",
      "types": "./dist/sources/fec/index.d.ts"
    }
  },
  "dependencies": {
    "axios": "^1.6.7",
    "p-queue": "^8.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  }
}
